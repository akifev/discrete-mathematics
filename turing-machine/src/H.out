start: IF
accept: AC
reject: RJ
blank: _

IF ! -> INCREMENT ! <
IF 0 -> AC 0 ^
IF 1 -> SEPARATOR 1 <
IF 2 -> SEPARATOR 2 <

SEPARATOR _ -> INCREMENT ! <

END 0 -> END 0 >
END 1 -> END 1 >
END 2 -> END 2 >
END _ -> DECREMENT _ <
END ! -> END ! >

INCREMENT 0 -> END 1 ^
INCREMENT _ -> END 1 ^
INCREMENT 1 -> INCREMENT 0 <

DECREMENT 0 -> DECREMENT 2 <
DECREMENT 1 -> BEGIN 0 ^
DECREMENT 2 -> BEGIN 1 ^
DECREMENT ! -> BEGIN _ <

BEGIN 0 -> BEGIN 0 <
BEGIN 1 -> BEGIN 1 <
BEGIN 2 -> BEGIN 2 <
BEGIN ! -> REDUCE ! >
BEGIN _ -> AC _ >

REDUCE 0 -> DELETE_LAST 2 >
REDUCE 1 -> IF 1 <
REDUCE 2 -> IF 2 <
REDUCE _ -> DECREMENT _ <

DELETE_LAST 2 -> DELETE_LAST 2 >
DELETE_LAST _ -> DELETE _ <

DELETE 2 -> BEGIN _ <